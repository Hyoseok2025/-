{
  "version": 2,
  "builds": [
    {
      "src": "api/**/*.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/api/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/$1"
    }
  ],
  "env": {
    "GEMINI_API_URL": "@gemini-api-url",
    "GEMINI_ACCESS_TOKEN": "@gemini-access-token",
    "FORCE_DEMO": "false"
  }
}
